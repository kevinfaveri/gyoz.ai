{
  "name": "gyoza",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server/index.ts --outdir=./",
    "dev": "remix dev -c \"npm run dev:server\" --manual",
    "dev:server": "tsx watch --clear-screen=false --ignore 'app/**' --ignore 'build/**' --ignore 'node_modules/**' --inspect ./server/index.ts",
    "start": "cross-env NODE_ENV=production node server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.9",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
