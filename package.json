  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
    "@remix-run/eslint-config": "^2.9.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/source-map-support": "^0.5.10",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.6.0",
